    </form>

    <h2>Register</h2>

    <form class="login" action="/users" method="post">
      <label for="">Email</label>
      <input type="text" name="email" value="">
      <label for="">Password</label>
      <input type="password" name="password" value="">
      <button>Sign Up</button>

    </form>




  </body>
</html>
